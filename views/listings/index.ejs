
// This is the index page for listings where users can view all listings.
// CRUD Operation: Read (we perform read operation here).
// We also provide a button to create a new listing.


<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>WanderLust</title>
</head>

<body>
  <h3>All Listings</h3>
  <form method="GET" action="/listings/new">
    <button>Create New Listing</button>
  </form>
  <ul>
    <% for(let listing of allListings) { %>
      <li><a href="/listings/<%= listing._id %>">
          <%= listing.title %>
        </a></li>
      <% } %>
  </ul>
</body>

</html>